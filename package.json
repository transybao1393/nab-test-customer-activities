    "body-parser": "^1.18.3",
    "busboy": "^0.2.14",
    "chance": "^1.0.16",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "helmet": "^3.15.0",
    "husky": "^1.0.1",
    "jsonwebtoken": "^8.4.0",
    "kue": "^0.11.6",
    "lodash": "^4.17.15",
    "memoizee": "^0.4.14",
    "moment": "^2.22.2",
    "mongoose": "^5.3.4",
    "morgan": "^1.9.0",
    "nodemailer": "^4.7.0",
