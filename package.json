    "@babel/plugin-transform-block-scoping": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-stage-2": "^7.8.3",
    "amqplib": "^0.5.6",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.1.0",
